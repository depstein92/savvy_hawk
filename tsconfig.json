{
    "compilerOptions": {
        "outDir": "./dist/",
        "sourceMap": true,
        "noImplicitAny": true,
        "module": "commonjs",
        "target": "es5",
        "jsx": "react",
    		"target": "esnext",
    		"moduleResolution": "node",
    		"allowJs": true,
    		"noEmit": true,
    		"strict": true,
    		"isolatedModules": true,
    		"esModuleInterop": true
    },
    "include": [
        "./src/**/*"
    ]
}
